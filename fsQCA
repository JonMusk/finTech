library(readxl)
data = read_excel("C:\\Users\\Administrator\\Desktop\\fsQCAData.xlsx")
View(data)
data = na.omit(data)
library(QCA)
NCSKEW = calibrate(data$NCSKEW, thresholds = findTh(data$NCSKEW,3))
FintechScore = calibrate(data$fintechScore, thresholds = findTh(data$fintechScore,3))
hzESG = calibrate(data$hzESG, thresholds = findTh(data$hzESG,3))
TotalAssets = calibrate(data$TotalAssets, thresholds = findTh(data$TotalAssets,3))
AssetLiabilityRatio=calibrate(data$AssetLiabilityRatio,thresholds=findTh(data$AssetLiabilityRatio,3))
ToverOsY = calibrate(data$ToverOsY,thresholds = findTh(data$ToverOsY,3))
calibrateData = data.frame(NCSKEW,FintechScore,hzESG,TotalAssets,AssetLiabilityRatio,ToverOsY)
View(calibrateData)
superSubset(calibrateData,outcome = "NCSKEW", incl.cut = 0.9)
pof("FintechScore<=NCSKEW",data = calibrateData)
pof("FintechScore<=~NCSKEW",data = calibrateData)
pof("~FintechScore<=~NCSKEW",data = calibrateData)
pof("~FintechScore<=NCSKEW",data = calibrateData)
pof("hzESG<=NCSKEW",data = calibrateData)
pof("hzESG<=~NCSKEW",data = calibrateData)
pof("~hzESG<=~NCSKEW",data = calibrateData)
pof("~hzESG<=NCSKEW",data = calibrateData)
pof("AssetLiabilityRatio<=NCSKEW",data = calibrateData)
pof("AssetLiabilityRatio<=~NCSKEW",data = calibrateData)
pof("~AssetLiabilityRatio<=NCSKEW",data = calibrateData)
pof("~AssetLiabilityRatio<=~NCSKEW",data = calibrateData)
pof("ToverOsY<=NCSKEW",data = calibrateData)
pof("ToverOsY<=~NCSKEW",data = calibrateData)
pof("~ToverOsY<=NCSKEW",data = calibrateData)
pof("~ToverOsY<=~NCSKEW",data = calibrateData)
pof("TotalAssets<=NCSKEW",data = calibrateData)
pof("TotalAssets<=~NCSKEW",data = calibrateData)
pof("~TotalAssets<=NCSKEW",data = calibrateData)
pof("~TotalAssets<=~NCSKEW",data = calibrateData) 
tt=truthTable(calibrateData,outcome="NCSKEW",conditions="FintechScore,hzESG,TotalAssets,AssetLiabilityRatio,ToverOsY",incl.cut=0.9,complete=TRUE,n.cut=5)
minimize(tt, include = "?", details = TRUE)
minimize(tt, include = "?",  dir.exp = "1,1,1,-,-",  details = TRUE)
tt=truthTable(calibrateData,outcome="NCSKEW",conditions="FintechScore,hzESG,TotalAssets,AssetLiabilityRatio,ToverOsM",incl.cut=0.85,complete=TRUE,n.cut=5)
minimize(tt, include = "?", details = TRUE)
minimize(tt, include = "?",  dir.exp = "1,1,1,-,-",  details = TRUE)
minimize(tt, include = "?", exclude = findRows(obj = tt, type =2), dir.exp = "1,1,1,-,-",details = TRUE)
library(SetMethods)
minimize(tt, include = "?", exclude = findRows(obj = tt, type =2), dir.exp = "1,1,1,0,0",details = TRUE)
minimize(tt, include = "?",  dir.exp = "1,1,1,0,0",details = TRUE)
minimize(tt, include = "?",  dir.exp = "1,1,1,-,-",details = TRUE)
sol = minimize(tt, include = "?",  dir.exp = "1,1,1,-,-",details = TRUE)
cluster(results=sol,data=calibrateData,outcome="NCSKEW",unit_id="data.company_code",cluster_id = "data.year")
tt=truthTable(calibrateData,outcome="NCSKEW",conditions="FintechScore,hzESG,TotalAssets,AssetLiabilityRatio,ToverOsY",incl.cut=0.85,complete=TRUE,n.cut = 5)
minimize(tt, include = "?", dir.exp = "1,1,1,-,-",details = TRUE)
